<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xogrp.recyclerviewadapterdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.albert.recyclerviewadapterdemo.WrapperAdapterActivity"></activity>
        <activity android:name="com.albert.recyclerviewadapterdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.albert.recyclerviewadapterdemo.MultiChoiceActivity"
            android:label="MultiChoice" />
        <activity
            android:name="com.albert.recyclerviewadapterdemo.HeaderAndFooterActivity"
            android:label="HeaderAndFooter" />
        <activity
            android:name="com.albert.recyclerviewadapterdemo.SpareArrayActivity"
            android:label="SpareArray" />
        <activity
            android:name="com.albert.recyclerviewadapterdemo.DecorationActivity"
            android:label="DecorationActivity" />
        <activity
            android:name="com.albert.recyclerviewadapterdemo.LoadMoreActivity"
            android:label="LoadMoreActivity" />
    </application>

</manifest>